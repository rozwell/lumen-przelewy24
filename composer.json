{
    "name": "rozwell/lumen-przelewy24",
    "description": "Lumen przelewy24.pl integration",
    "type": "library",
    "require": {
        "php": ">=7.1.0",
        "laravel/lumen-framework": ">=5.0",
        "guzzlehttp/guzzle": ">=6.3",
        "ramsey/uuid": "^3 || ^4",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Devpark\\Transfers24\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpunit/phpunit": "^5.6",
        "mockery/mockery": "^0.9.6"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Devpark\\Transfers24\\Providers\\Transfers24ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
